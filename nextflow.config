docker.enabled = true

workflow.output.mode = 'copy'

params {
    num_dangling_bases = 1
    make_output_bam = true
}

process {
    withLabel: 'gatk' {
        container = 'docker.io/broadinstitute/gatk:4.6.2.0'
    }

    withLabel: 'bwa_and_picard' {
        container = ''
    }

    withLabel: 'crossmap' {
        container = ''
    }
}